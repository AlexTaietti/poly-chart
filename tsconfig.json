{
   "compilerOptions": {
      "strictPropertyInitialization": false,
      "allowSyntheticDefaultImports": true,
      "noFallthroughCasesInSwitch": true,
      "noUnusedParameters": true,
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "strict": true,
      "allowJs": true,
      "jsx": "react",
      "outDir": "$$type$$"
   },
   "include": [
      "src"
   ],
   "exclude": [
      "node_modules"
   ]
}